{
  "dependencies": {
    "@openrouter/ai-sdk-provider": "^0.7.2",
    "@types/cron": "^2.4.3",
    "ai": "^4.3.16",
    "cron": "^4.3.1",
    "discord.js-selfbot-v13": "^3.6.1",
    "dotenv": "^17.0.0",
    "tsx": "^4.20.3",
    "zod": "^3.25.67",
    "drizzle-orm": "^0.44.2",
    "pg": "^8.16.2",
    "redis": "^4.7.0"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tspc",
    "start": "node dist/src/index.js",
    "format": "prettier --write .",
    "typecheck": "tspc --noEmit",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint src/**/*.ts",
    "lint:prettier": "prettier --check .",
    "db:up": "docker compose -f docker/db.compose.yml up -d",
    "db:down": "docker compose -f docker/db.compose.yml down",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate && prettier --write database/migrations",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push"
  },
  "devDependencies": {
    "@types/node": "^24.0.7",
    "@eslint/js": "^9.22.0",
    "@types/pg": "^8.15.4",
    "@types/redis": "^4.0.10",
    "drizzle-kit": "^0.31.1",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "ts-patch": "^3.3.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "typescript-transform-paths": "^3.5.3"
  }
}
